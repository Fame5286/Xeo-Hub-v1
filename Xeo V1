local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Xeo Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})




local Tab = Window:MakeTab({
    Name = "FARM",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

Tab:AddToggle({
    Name = "AUTO FARM",
    Default = false,
    Callback = function(Value)
    pcall(function()
        _G. Teleport = Value 
while _G. Teleport do wait ()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-442061.25, 2.9980247020721436, 8.44399642944336)
wait(.7)
        end
    end)
end
})

----

Tab:AddToggle({
    Name = "AUTO Click",
    Default = false,
    Callback = function(Value)
        _G.Auto = Value
while _G.Auto do wait()
    game:GetService("ReplicatedStorage").Packages.Knit.Services.ClickService.RF.Click:InvokeServer()

        end
end
})

----

Tab:AddToggle({
    Name = "AUTO W",
    Default = false,
    Callback = function(Value)
        _G.Auto = Value
while _G.Auto do wait()
game:GetService("VirtualInputManager"):SendKeyEvent(true,119,false,game.Players.LocalPlayer.Character.HumanoidRootPart)
        end
end
})


----

local Tab = Window:MakeTab({
    Name = "SPIN",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

Tab:AddToggle({
    Name = "SPIN 7.5K",
    Default = false,
    Callback = function(Value)
        _G. Open = Value
while _G. Open do wait ()
wait(.0)
local args = {
    [1] = "Pro01",
    [2] = "Auto",
    [3] = {}
}

game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("Open"):InvokeServer(unpack(args))

     end
end
})

----

