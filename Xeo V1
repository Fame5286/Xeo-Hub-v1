local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Xeo Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})




local Tab = Window:MakeTab({
    Name = "FARM",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

Tab:AddToggle({
    Name = "AUTO FARM",
    Default = false,
    Callback = function(Value)
        _G. Teleport = Value 
while _G. Teleport do wait ()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-442061.25, 2.9980247020721436, 8.44399642944336)
wait(.9)
        end
end
})

----

Tab:AddToggle({
    Name = "AUTO Click",
    Default = false,
    Callback = function(Value)
        _G.Auto = Value
while _G.Auto do wait()
    game:GetService("ReplicatedStorage").Packages.Knit.Services.ClickService.RF.Click:InvokeServer()

        end
end
})

----

Tab:AddToggle({
    Name = "copy",
    Default = false,
    Callback = function(Value)
        _G.copy = Value
while _G.copy do wait()
   setclipboard(tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.Position))

        end
end
})

----





OrionLib:Init()
